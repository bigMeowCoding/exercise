{
  "extends": "eslint:recommended",
  "rules": {
    "no-var": "error",
    "semi": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true // 不加promise使用会报错
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}